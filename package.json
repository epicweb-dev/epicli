{
  "name": "epicli",
  "version": "0.0.0-semantically-released",
  "description": "A powerful CLI for working with the Epic Stack",
  "main": "dist/index.js",
  "type": "module",
  "bin": {
    "epicli": "dist/bin/epicli.js"
  },
  "scripts": {
    "start": "node dist/bin/epicli.js",
    "build": "tsc -p tsconfig.src.json",
    "dev": "tsx ./src/bin/epicli.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "test": "vitest"
  },
  "keywords": [
    "epic-stack",
    "cli",
    "ai",
    "prompts"
  ],
  "author": "Kent C. Dodds <me@kentcdodds.com> (https://kentcdodds.com/)",
  "license": "MIT",
  "prettier": "@epic-web/config/prettier",
  "dependencies": {
    "@epic-web/invariant": "^1.0.0",
    "chalk": "^5.4.1",
    "clipboardy": "^4.0.0",
    "figlet": "^1.8.0",
    "filenamify": "^6.0.0",
    "gradient-string": "^3.0.0",
    "minimatch": "^10.0.1",
    "octokit": "^4.1.2",
    "prompts": "^2.4.2",
    "simple-git": "^3.27.0",
    "yargs": "^17.7.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@epic-web/config": "^1.18.2",
    "@types/figlet": "^1.7.0",
    "@types/gradient-string": "^1.1.6",
    "@types/node": "^22.13.11",
    "@types/prompts": "^2.4.9",
    "@types/yargs": "^17.0.33",
    "eslint": "^9.23.0",
    "prettier": "^3.5.3",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2",
    "vitest": "^3.0.9"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/epicweb-dev/epicli"
  },
  "bugs": {
    "url": "https://github.com/epicweb-dev/epicli/issues"
  },
  "homepage": "https://github.com/epicweb-dev/epicli#readme"
}
